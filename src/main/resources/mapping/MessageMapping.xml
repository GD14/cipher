<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 命名空间 是必须的 -->
<mapper namespace="com.rong.ssm.mapper.MessageMapper">
    <select id="selectByCallingId" parameterType="int" resultType="com.rong.ssm.pojo.Message">
        select * from `message` where calling_id=#{id}
    </select>
    <select id="selectByCallingPhone" parameterType="String" resultType="com.rong.ssm.pojo.Message">
        select * from `message` where calling=#{id}
    </select>
</mapper>